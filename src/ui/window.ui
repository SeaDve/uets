<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="UetsWindow" parent="AdwApplicationWindow">
    <property name="width-request">1024</property>
    <property name="height-request">600</property>
    <property name="default-width">1024</property>
    <property name="default-height">600</property>
    <property name="content">
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child type="top">
          <object class="AdwHeaderBar">
          </object>
        </child>
        <child>
          <object class="GtkScrolledWindow">
            <property name="vexpand">True</property>
            <property name="child">
              <object class="GtkBox">
                <property name="orientation">vertical</property>
                <property name="margin-start">12</property>
                <property name="margin-end">12</property>
                <property name="margin-top">12</property>
                <property name="margin-bottom">12</property>
                <child>
                  <object class="GtkEntry" id="test_entry">
                  </object>
                </child>
                <child>
                  <object class="AdwPreferencesGroup">
                    <property name="title">All</property>
                    <child>
                      <object class="GtkListBox" id="test_all_listbox">
                        <property name="selection-mode">none</property>
                        <style>
                          <class name="boxed-list"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwPreferencesGroup">
                    <property name="title">Inside</property>
                    <child>
                      <object class="GtkListBox" id="test_inside_listbox">
                        <property name="selection-mode">none</property>
                        <style>
                          <class name="boxed-list"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
