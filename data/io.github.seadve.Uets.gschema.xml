<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema path="/io/github/seadve/Uets/" id="io.github.seadve.Uets">
    <key name="operation-mode" type="s">
      <choices>
        <choice value="counter"/>
        <choice value="attendance"/>
        <choice value="parking"/>
        <choice value="inventory"/>
        <choice value="refrigerator"/>
      </choices>
      <default>"counter"</default>
    </key>
    <key name="camera-ip-addr" type="s">
      <default>"192.168.100.159"</default>
    </key>
    <key name="aux-camera-ip-addrs" type="as">
      <default>[]</default>
    </key>
    <key name="rfid-reader-ip-addr" type="s">
      <default>"uets-rfid-reader.local"</default>
    </key>
    <key name="relay-ip-addr" type="s">
      <default>"uets-relay.local"</default>
    </key>
    <key name="enable-lower-limit-reached-alert" type="b">
      <default>false</default>
    </key>
    <key name="lower-limit-reached-threshold" type="u">
      <default>0</default>
    </key>
    <key name="enable-upper-limit-reached-alert" type="b">
      <default>false</default>
    </key>
    <key name="upper-limit-reached-threshold" type="u">
      <default>4294967295</default>
    </key>
    <key name="enable-detection-wo-id" type="b">
      <default>true</default>
    </key>
    <key name="enable-n-inside-hook" type="b">
      <default>true</default>
    </key>
    <key name="n-inside-hook-threshold" type="u">
      <default>0</default>
    </key>
    <key name="max-entry-to-exit-duration-secs" type="u">
      <default>4294967295</default>
    </key>
  </schema>
</schemalist>
