<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="UetsEntryDialog" parent="AdwDialog">
    <property name="follows-content-size">True</property>
    <property name="presentation-mode">bottom-sheet</property>
    <property name="child">
      <object class="AdwToolbarView">
        <child type="top">
          <object class="AdwHeaderBar">
            <property name="show-start-title-buttons">False</property>
            <property name="show-end-title-buttons">False</property>
            <property name="title-widget">
              <object class="AdwWindowTitle">
                <property name="title">Enter Data</property>
              </object>
            </property>
          </object>
        </child>
        <property name="content">
          <object class="AdwPreferencesPage">
            <child>
              <object class="AdwPreferencesGroup">
                <child>
                  <object class="AdwActionRow" id="stock_id_row">
                    <property name="title">Stock Name</property>
                    <child type="suffix">
                      <object class="GtkDropDown" id="stock_id_dropdown">
                        <property name="valign">center</property>
                        <property name="show-arrow">False</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwEntryRow" id="location_row">
                    <property name="title">Location</property>
                  </object>
                </child>
                <child>
                  <object class="AdwEntryRow" id="expiration_dt_row">
                    <property name="title">Expiration Date</property>
                  </object>
                </child>
                <child>
                  <object class="AdwEntryRow" id="name_row">
                    <property name="title">Name</property>
                  </object>
                </child>
                <child>
                  <object class="AdwComboRow" id="sex_row">
                    <property name="title">Sex</property>
                    <property name="model">
                      <object class="GtkStringList">
                        <items>
                          <item>Male</item>
                          <item>Female</item>
                        </items>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="AdwEntryRow" id="email_row">
                    <property name="title">Email</property>
                  </object>
                </child>
                <child>
                  <object class="AdwEntryRow" id="program_row">
                    <property name="title">Program</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </property>
        <child type="bottom">
          <object class="GtkBox">
            <property name="margin-top">12</property>
            <property name="margin-bottom">12</property>
            <property name="halign">center</property>
            <property name="spacing">12</property>
            <child>
              <object class="GtkButton">
                <property name="action-name">entry-window.cancel</property>
                <property name="label">Cancel</property>
                <style>
                  <class name="pill"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="action-name">entry-window.done</property>
                <property name="label">Done</property>
                <style>
                  <class name="suggested-action"/>
                  <class name="pill"/>
                </style>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
